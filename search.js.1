$(document).ready(function(){
  $(".search__submit").click(function(){
       var category =  $('.search__input').attr('categoryText'); 
       var searchKey = $('.search__input').val();
	   var dataKey = $('.search__input').attr('data-placeholder');
	   if(searchKey == dataKey) {
		   searchKey = "Search News";
	   }		   
       window.location = '/allnews.html?searchkey='+searchKey+'&category='+category+'&month=all&page=1';
    });
});